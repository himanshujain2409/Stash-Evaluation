<apex:page standardController="Signuprequest" extensions="SignupRequestExt">
<apex:form >
  <apex:pageMessages />
  <apex:pageBlock title="Sign Up Request" mode="edit" >
  <apex:pageBlockSection title="Detail Page" >
     <apex:pageBlockSectionItem >
        <apex:outputLabel value="{!$ObjectType.Signuprequest.fields.FirstName.label}"  />
         <apex:inputField value="{!Signuprequest.FirstName}" />
      </apex:pageBlockSectionItem>
      <apex:pageBlockSectionItem >
       <apex:outputLabel value="{!$ObjectType.Signuprequest.fields.LastName.label}"  />
        <apex:inputField value="{!Signuprequest.LastName}" />
     </apex:pageBlockSectionItem>
      <apex:pageBlockSectionItem >
       <apex:outputLabel value="{!$ObjectType.Signuprequest.fields.Country.label}"  />
        <apex:inputField value="{!Signuprequest.Country}" />
     </apex:pageBlockSectionItem>
     <apex:pageBlockSectionItem >
       <apex:outputLabel value="{!$ObjectType.Signuprequest.fields.Company.label}"  />
        <apex:inputField value="{!Signuprequest.Company}" />
     </apex:pageBlockSectionItem>
      <apex:pageBlockSectionItem >
       <apex:outputLabel value="{!$ObjectType.Signuprequest.fields.SignupEmail.label}"  />
        <apex:inputField value="{!Signuprequest.SignupEmail}" />
     </apex:pageBlockSectionItem>
      <apex:pageBlockSectionItem >
       <apex:outputLabel value="{!$ObjectType.Signuprequest.fields.Username.label}"  />
        <apex:inputField value="{!Signuprequest.Username}" />
     </apex:pageBlockSectionItem> 
      <apex:pageBlockSectionItem >
       <apex:outputLabel value="{!$ObjectType.Signuprequest.fields.Service_Request__c.label}"  />
        <apex:inputField value="{!Signuprequest.Service_Request__c}"  />
     </apex:pageBlockSectionItem>
      <apex:pageBlockSectionItem >
       <apex:outputLabel value="{!$ObjectType.Signuprequest.fields.Industry__c.label}"  />
        <apex:inputField value="{!Signuprequest.Industry__c}" required="true"/>
     </apex:pageBlockSectionItem>
     
     <apex:pageBlockSectionItem >
       <apex:outputLabel value="Source template"  />
       <apex:selectList value="{!Signuprequest.TemplateId}" required="true">
            <apex:selectOptions value="{!items}"/>

        </apex:selectList>
     </apex:pageBlockSectionItem>
     
     <apex:pageBlockSectionItem >
       <apex:outputLabel value="{!$ObjectType.Signuprequest.fields.Source_MDO__c.label}"  />
       <apex:inputField value="{!Signuprequest.Source_MDO__c}" />
     </apex:pageBlockSectionItem>

     <apex:pageBlockSectionItem >
       <apex:outputLabel value="{!$ObjectType.Signuprequest.fields.Description__c.label}"  />
       <apex:inputField value="{!Signuprequest.Description__c}" />
     </apex:pageBlockSectionItem>
     
        
  </apex:pageBlockSection>
   <apex:pageBlockButtons >
                 <apex:commandButton action="{!save}" value="Save" id="theButton"/>
   </apex:pageBlockButtons>

  </apex:pageBlock>
  </apex:form>
</apex:page>