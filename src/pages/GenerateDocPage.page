      <apex:actionstatus id="status" starttext="Please Wait......" stoptext=""></apex:actionstatus>
        <apex:actionFunction name="generateDocument"  status="status" action="{!doGenerateDoc}" reRender="test" >
                 <apex:param name="sessionId" assignTo="{!apiSessionId}" value="" />
         <apex:param name="serverURL" assignTo="{!apiServerURL}" value="" />
        </apex:actionFunction> 
        
    </apex:form>
