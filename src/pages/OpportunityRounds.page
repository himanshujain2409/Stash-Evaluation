<apex:page standardController="Opportunity" extensions="OpportunityRoundsController">
    <apex:pageBlock >
        <apex:pageBlockTable value="{!rounds}" var="round">
            <apex:column headerValue="Round Name"> 
                <apex:outputLink value="/{!round.Id}" target="_parent">
                    {!round.name}
                </apex:outputLink>
            </apex:column> 
            
            <apex:column headerValue="Next Step" value="{!round.Task__c}" />
            
            <apex:column headerValue="Callback Date" value="{!round.Callback_Date__c}" /> 
            
            <apex:column headerValue="KP EM" value="{!round.KP_EM__c}" /> 
            
            <apex:column headerValue="KP VM" value="{!round.KP_VM__c}" /> 
            
            <apex:column headerValue="Admin Dialogue" value="{!round.Admin_Dialogue__c}" /> 
            
            <apex:column headerValue="KP Dialogue" value="{!round.KP_Dialogue__c}" /> 
            
            <apex:column headerValue="Admin EM" value="{!round.Admin_EM__c}" /> 
            
            <apex:column headerValue="Admin VM" value="{!round.Admin_VM__c}" />  
            
            <apex:column headerValue="Note" value="{!round.Note__c}" /> 
            
        </apex:pageBlockTable>
    </apex:pageBlock>  
</apex:page>