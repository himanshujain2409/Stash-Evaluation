<apex:page standardController="Apttus__APTS_Agreement__c" action="{!doInit}" extensions="APTPS_SendForReviewController">
     <apex:form >
     <apex:pageBlock >
         <apex:pageblockButtons >
             <apex:commandButton value="Generate" action="{!generateDocument}"/>
             <apex:commandButton value="Cancel" action="{!GoBack}"/>
         </apex:pageblockButtons>
     
         <apex:pageblocksection columns="1">
         
             <apex:pageBlockTable value="{!listOfAttachmentWrapper}" var="item" >
             
                     <apex:column >
                         <apex:inputCheckbox value="{!item.isChecked}"/>
                     </apex:column>
                     <apex:column value="{!item.attachment.name}"/>
                   
             
             </apex:pageblockTable>
         
         </apex:pageblocksection>
         
     </apex:pageBlock>
     </apex:form>
</apex:page>